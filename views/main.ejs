<html>

	<head>

		<meta charset="utf-8">
		<meta name="description" content="Fun and powerful chat platform with dynamic images and internet radio.">

		<title><%= vars.default_title %></title>
		<script src="/js/jquery.min.js"></script>
		<script src="/js/handlebars.js"></script>
		<script src="/js/dropzone.js"></script>
		<script src="/js/dateformat.js"></script>
		<script src="/js/perfect-scrollbar.min.js?v=3"></script>
		<script src="/js/nicescroll.min.js"></script>
		<script src="/js/tippy.all.min.js"></script>
		<script src="/js/spectrum.min.js"></script>
		<script src="/js/cropper.min.js"></script>
		<script type="text/javascript" src="/js/contextmenu/jquery.contextMenu.min.js"></script>
		<script src="https://player.twitch.tv/js/embed/v1.js"></script>
		<script src="/js/contextmenu/jquery.ui.position.min.js"></script>
		<script src="/socket.io/socket.io.js"></script>
		<script src="/js/hls.js"></script>

		<link rel="stylesheet" type="text/css" href="/css/perfect-scrollbar.css?v=3"/>
		<link rel="stylesheet" href="/css/font-awesome/css/font-awesome.min.css">
		<link rel="stylesheet" href="/css/cropper.min.css">
		<link rel="stylesheet" href="/css/spectrum.css">
		<link rel="stylesheet" type="text/css" href="/js/contextmenu/jquery.contextMenu.min.css"> 

		<script src="/js/msg.min.js?v=12"></script>
		<script src="/js/utilz.js?ver=1"></script>
		<script src="/js/colorlib.js?v=7"></script>

		<script src="/js/base_o.js?ver=799"></script>

		<link rel="stylesheet" type="text/css" href="/css/style.css?ver=234"/>

		<script>

			<% 
				for(var key in vars)
				{
					var v = vars[key]

					%><%-key%> = <%

					if(typeof v === "string")
					{
						%>`<%-v%>`; <% 
					}

					else
					{
						%><%-v%>; <%
					}
				}


			%>

			$(function()
			{
				init()

				var script = document.createElement('script')
				script.src = "https://www.youtube.com/iframe_api"
				document.head.appendChild(script)
			})

		</script>

	</head>

	<body>

		<div id="main_container">

			<div id='background_image'></div>

			<div id="main_flex">

				<div id='header' class='unselectable bg2'>

					<div id='header_items'>

						<div id='header_items_inner'>

							<i id='main_menu' class='header_item icon fa fa-th-large nicetitle' onclick='show_main_menu()' title="Main Menu"></i>
							<span class='hspacer1'></span>|<span class='hspacer1'></span>
							<span class='header_item' id='usercount' onclick='show_userlist()'></span>

							<span id='radio'>
								
								<span class='hspacer1'></span>|<span class='hspacer1'></span>

								<span class='header_item' id='toggle_radio_state' onclick='toggle_radio_state()'>
									<i class='icon fa fa-volume-up' id='playing_icon'></i>
									<span id='toggle_now_playing_text'>Start Radio</span>
								</span>

								<span id='volume_area'>
									<span class='hspacer1'></span>|<span class='hspacer1'></span>
									<span id='volume_controls'>
										<i id='header_icon_minus' class='header_item icon fa fa-minus' onclick='volume_decrease()'></i>
										<span class='header_item' onclick='volume_decrease()'>Volume:</span>
										<span class='header_item' id='volume' onclick='volume_increase()'>100%</span>
										<i id='header_icon_plus' class='header_item icon fa fa-plus' onclick='volume_increase()'></i>
									</span>
								</span>

								<span id="now_playing_area" onclick="show_played()">
									<span class='hspacer1'></span>|<span class='hspacer1'></span> 
									<span id='now_playing_controls'>
										<span class='header_item'>Now Playing:</span>
										<span class='header_item' id='now_playing'>Loading</span>
									</span>
								</span>

							</span>

							<span id='header_topic'>
								<span class='hspacer1'></span>|<span class='hspacer1'></span>
								<span class='header_item'>Topic:</span>
								<span class='header_item' id='header_topic_text'></span>
							</span>

						</div>

					</div>

				</div>

				<div id="flex_columns">

					<div id='chat_main'>

						<div id='chat_area_parent'>
							<div id='chat_area' class='bg1'></div>
						</div>
						
						<div id='scroll_notice' class='unselectable' onclick='goto_bottom(true)'>Go To Bottom</div>
					</div>

					<div id='media' class='unselectable bg1' draggable='false'>

						<div id='media_split'>
							
							<div id='media_image_container'>
								<img id='media_image' draggable='false' onclick="show_current_image_modal()">
							</div>

							<div id='media_tv'>
							
								<div id='media_twitch_video_container'></div>

								<div id='media_youtube_video_container'>
									<div id='media_youtube_video' class='video_frame'></div>	
								</div>

								<div id='media_video_container'>
									<video id='media_video' class='video_frame' width="640px" height="360px" controls></video>
								</div>

							</div>

						</div>

					</div>

				</div>

				<div id='footer' class='bg2'>
					<div id='footer_items'>

						<span id='footer_userinfo' class='fa fa-navicon footer_icon nicetitle' onClick="show_userinfo()" title="User Menu"></span>

						<input type='text' id='input'>

						<span class='unselectable'>

							<span id='toggle_images_text' class='footer_text'>Images</span>
							<i class='fa fa-toggle-on footer_icon2 nicetitle' id='footer_toggle_images_icon' onclick='toggle_images()' title='Enable/Disable Images. Disabled means it will be invisible and inactive'></i>
							<i class='fa fa-unlock-alt footer_icon nicetitle' id='footer_lock_images_icon' onclick='toggle_lock_images()' title='Lock/Unlock Images. Locked means it will stay the same even if media is changed by a user'></i>
 
							<span class='hspacer1'></span>
 
							<span id='toggle_tv_text' class='footer_text'>TV</span>
							<i class='fa fa-toggle-on footer_icon2 nicetitle' id='footer_toggle_tv_icon' onclick='toggle_tv()' title='Enable/Disable TV. Disabled means it will be invisible and inactive'></i>
							<i class='fa fa-unlock-alt footer_icon nicetitle' id='footer_lock_tv_icon' onclick='toggle_lock_tv()' title='Lock/Unlock TV. Locked means it will stay the same even if media is changed by a user'></i>
 
							<span class='hspacer1'></span>
 
							<span id='toggle_radio_text' class='footer_text'>Radio</span>
							<i class='fa fa-toggle-on footer_icon2 nicetitle' id='footer_toggle_radio_icon' onclick='toggle_radio()' title='Enable/Disable Radio. Disabled means it will be invisible and inactive'></i>
							<i class='fa fa-unlock-alt footer_icon nicetitle' id='footer_lock_radio_icon' onclick='toggle_lock_radio()' title='Lock/Unlock Radio. Locked means it will stay the same even if media is changed by a user'></i>
 
							<span class='hspacer1'></span>

							<i class='fa fa-camera footer_icon nicetitle' id='footer_upload_icon' title='Change the Image' onclick='show_image_picker()'></i>
							<i class='fa fa-television footer_icon nicetitle' id='footer_tv_icon' onclick="show_tv_url_picker()" title='Change the TV'></i>
							<i class='fa fa-volume-up footer_icon nicetitle' id='footer_radio_icon' onclick="show_radio_url_picker()" title='Change the Radio'></i>

						</span>

					</div>
				</div>				

			</div>

			<img id='test_image'>

		</div>

		<script id='template_menu' type='text/x-handlebars-template'>
