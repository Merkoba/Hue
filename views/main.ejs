<% include doctype.ejs %>

<html>

	<head>

		<% include meta.ejs %>

		<title><%= vars.default_title %></title>

		<script src="/static/js/jquery.min.js"></script>
		<script src="/static/js/handlebars.js"></script>
		<script src="/static/js/dropzone.js"></script>
		<script src="/static/js/dateformat.js"></script>
		<script src="/static/js/perfect-scrollbar.min.js?version=3"></script>
		<script src="/static/js/nicescroll.min.js"></script>
		<script src="/static/js/tippy.all.min.js"></script>
		<script src="/static/js/spectrum.min.js"></script>
		<script src="/static/js/cropper.min.js?version=2"></script>
		<script type="text/javascript" src="/static/js/contextmenu/jquery.contextMenu.min.js"></script>
		<script src="https://player.twitch.tv/js/embed/v1.js"></script>
		<script src="/static/js/contextmenu/jquery.ui.position.min.js"></script>
		<script src="/static/js/socket.io.js"></script>
		<script src="/static/js/hls.js"></script>
		<script src="https://w.soundcloud.com/player/api.js"></script>
		<script src="/static/js/nstslider/jquery.nstSlider.min.js"></script>

		<link rel="stylesheet" type="text/css" href="/static/css/perfect-scrollbar.css?version=3"/>
		<link rel="stylesheet" href="/static/css/font-awesome/css/font-awesome.min.css">
		<link rel="stylesheet" href="/static/css/cropper.min.css?version=2">
		<link rel="stylesheet" href="/static/css/spectrum.css">
		<link rel="stylesheet" href="/static/css/spectrum.css">
		<link rel="stylesheet" href="/static/js/contextmenu/jquery.contextMenu.min.css"> 
		<link rel="stylesheet" href="/static/js/nstslider/jquery.nstSlider.min.css"> 

		<script src="/static/js/msg.min.js?version=26"></script>
		<script src="/static/js/utilz.js?version=16"></script>
		<script src="/static/js/colorlib.js?version=10"></script>

		<script src="/static/js/base.js?version=2172"></script>

		<link rel="stylesheet" href="/static/css/style.css?version=440"/>

		<script>

			<% include vars.ejs %>

			$(function()
			{
				init()

				var script = document.createElement('script')
				script.src = "https://www.youtube.com/iframe_api"
				document.head.appendChild(script)
			})

		</script>

	</head>

	<body>

		<div id="loading">
			<div id="loading_spinner"></div>
		</div>

		<div id="main_container">

			<div id='background_image' class='background_image'></div>

			<div class="columns_container">

				<div id='voice_chat_container' class='bg2'>
					<div id='voice_chat'></div>
				</div>

				<div id='header' class='unselectable bg2 panel'>

					<div id='header_items'>

						<div id='header_items_inner'>

							<div id='main_menu_icon' class='pointer nicetitle action' onclick='show_main_menu()' title="Main Menu">
								<div class='action pointer'><i class='icon fa fa-th-large'></i></div>
							</div>

							<div class='header_separator'>|</div>
							<div class='pointer action' id='usercount' onclick='show_userlist()'></div>

							<div id="header_voice_chat_container" class="pointer action nicetitle" title="Join/Leave Voice Chat" onclick="toggle_join_voice_chat()">
								<div class='header_separator'>|</div>
								<div>Voice Chat (<div id='voice_chat_num_users'></div>)</div>
							</div>

							<div id='header_radio'>
								
								<div class='header_separator'>|</div>

								<div class='pointer action' id='toggle_radio_state' onclick='toggle_radio_state()'>
									<div class='action pointer' id='header_radio_playing_icon'><i class='icon fa fa-volume-up'></i></div>
									<div id='toggle_now_playing_text'>Start Radio</div>
								</div>

								<div id='header_radio_volume_area'>
									<div class='header_separator'>|</div>
									<div id='header_radio_volume_controls'>
										<div id='header_radio_volume_icon_minus' class='action pointer' onclick='volume_decrease()'><i class='pointer icon fa fa-minus'></i></div>
										<div class='pointer action' onclick='volume_decrease()'>Volume:&nbsp;</div>
										<div class='pointer action' id='volume' onclick='volume_increase()'>100%</div>
										<div id='header_radio_volume_icon_plus' class='action pointer' onclick='volume_increase()'><i class='pointer icon fa fa-plus'></i></div>
									</div>
								</div>

								<div id='header_now_playing_area'>
									<div class='header_separator'>|</div> 
									<div id='header_now_playing_controls' class='action' onclick='show_played()'>
										<div class='pointer'>Now Playing:&nbsp;</div>
										<div class='pointer' id='now_playing'>Loading</div>
									</div>
								</div>

							</div>

							<div id='header_topic'>
								<div class='header_separator'>|</div>
								<div id='header_topic_inner' class='action' onclick='show_status()'>
									<div class='header_item'>Topic:&nbsp;</div>
									<div class='header_item' id='header_topic_text'></div>
								</div>
							</div>

						</div>

					</div>

				</div>

				<div class='unselectable' id='reactions_box_container'>
					<div id='reactions_box'>

						<i class='reaction_icon fa fa-thumbs-o-up pointer action nicetitle' onclick='send_reaction("like")' title="Like"></i>
						
						<div class='hspacer3'></div>
						
						<i class='reaction_icon fa fa-heart-o pointer action nicetitle' onclick='send_reaction("love")' title="Love"></i>

						<div class='hspacer3'></div>

						<i class='reaction_icon fa fa-smile-o pointer action nicetitle' onclick='send_reaction("happy")' title="Happy"></i>

						<div class='hspacer3'>&nbsp;|&nbsp;</div>

						<i class='reaction_icon fa fa-meh-o pointer action nicetitle' onclick='send_reaction("meh")' title="Meh"></i>

						<div class='hspacer3'></div>

						<i class='reaction_icon fa fa-frown-o pointer action nicetitle' onclick='send_reaction("sad")' title="Sad"></i>

						<div class='hspacer3'></div>

						<i class='reaction_icon fa fa-thumbs-o-down pointer action nicetitle' onclick='send_reaction("dislike")' title="Dislike"></i>
						
						<div class='hspacer3'>&nbsp;|&nbsp;</div>
						
						<div class='pointer action user_function_button' onclick='run_user_function(1)' id='user_function_button_1'>F1</div>

						<div class='hspacer3'>&nbsp;|&nbsp;</div>
						
						<div class='pointer action user_function_button' onclick='run_user_function(2)' id='user_function_button_2'>F2</div>

						<div class='hspacer3'>&nbsp;|&nbsp;</div>
						
						<div class='pointer action user_function_button' onclick='run_user_function(3)' id='user_function_button_3'>F3</div>

						<div class='hspacer3'>&nbsp;|&nbsp;</div>
						
						<div class='pointer action user_function_button' onclick='run_user_function(4)' id='user_function_button_4'>F4</div>
					</div>
				</div>

				<div class='unselectable action nicetitle pointer' id='topbox_container' onclick='lock_screen()' title="Lock Screen">
					<div id='topbox'>
						<i class='topbox_icon fa fa-lock'></i>
					</div>
				</div>

				<div class='rows_container' id="main_rows_container">

					<div id='chat_main' class='bg1'>

						<div class='scroller_container top_scroller_container' id='top_scroller_container'>
							<div id='top_scroller' class='left_scroller scroller action3 unselectable' onclick='goto_top()'>Go&nbsp;To&nbsp;Top</div>
							<div class='hspacer3'></div>
							<div id='up_scroller' class='right_scroller scroller action3 unselectable' onclick='activity_above()'>Activity&nbsp;Above</div>
						</div>

						<div id='chat_area_parent'>
							<div id='chat_area'></div>
						</div>

						<div class='scroller_container bottom_scroller_container' id='bottom_scroller_container'>
							<div id='bottom_scroller' class='left_scroller scroller action3 unselectable' onclick='goto_bottom(true)'>Go&nbsp;To&nbsp;Bottom</div>
							<div class='hspacer3'></div>
							<div id='down_scroller' class='right_scroller scroller action3 unselectable' onclick='activity_below()'>Activity&nbsp;Below</div>
						</div>

					</div>

					<div id='media' class='unselectable bg1' draggable='false'>

						<div id='media_split'>
							
							<div id='media_image_container'>
								
								<img id='media_image' class='pointer' draggable='false' onclick="show_current_image_modal()">
								<div id='media_image_error' class='pointer' onclick="show_current_image_modal()">Image failed to load</div>

								<div class='maxer_container'>
									<div id='media_image_maxer' class='maxer' onclick="maximize_images()"></div>
								</div>

							</div>

							<div id='media_tv'>

								<div id='media_iframe_video_container' class='media_video_container'>
									<iframe width="640px" height="360px" id='media_iframe_video' class='video_frame'></iframe>
								</div>

								<div id='media_soundcloud_video_container' class='media_video_container'>
									<iframe width="640px" height="360px" id='media_soundcloud_video' class='video_frame' src='https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/301986536'></iframe>
								</div>
							
								<div id='media_twitch_video_container' class='media_video_container'></div>

								<div id='media_youtube_video_container' class='media_video_container'>
									<div id='media_youtube_video' class='video_frame'></div>	
								</div>

								<div id='media_video_container' class='media_video_container'>
									<video id='media_video' class='video_frame' width="640px" height="360px" preload="metadata" poster="<%-vars.default_video_url%>" controls></video>
								</div>

								<div class='maxer_container'>
									<div id='media_tv_maxer' class='maxer' onclick="maximize_tv()"></div>
								</div>

							</div>

						</div>

					</div>

				</div>

				<div id='footer' class='bg2 panel'>
					<div id='footer_items'>

						<div id='footer_user_menu_container' class='pointer action' onclick="show_user_menu()">
							<i id='footer_user_menu' class='fa fa-user-circle footer_icon'></i>
						</div>

						<textarea id='input' size="1" rows="1"></textarea>

						<div class='unselectable'>

							<div id='footer_images_controls' class='footer_media_control'>

								<div class='footer_text_small'>Images</div>

								<div class='pointer action nicetitle' onclick='show_image_picker()' title="Change the Image">
									<i  id='footer_images_icon' class='fa fa-camera footer_icon2'></i>
								</div>

								<div class='pointer action nicetitle' onclick='toggle_images()' title='Show/Hide Images. Hidden means it will not be displayed or active'>
									<i id='footer_toggle_images_icon' class='fa fa-toggle-on footer_icon2'></i>
								</div>
								
								<div class='pointer action nicetitle' onclick='toggle_lock_images()' title='Lock/Unlock Images. Locked means it will stay the same even if media is changed by a user'>
									<i id='footer_lock_images_icon' class='fa fa-unlock-alt footer_icon'></i>
								</div>
							</div>
 	
							<div id='footer_tv_controls' class='footer_media_control'>

								<div class='footer_text_small'>TV</div>

								<div class='pointer action nicetitle' onclick='show_tv_picker()' title="Change the TV">
									<i id='footer_tv_icon' class='fa fa-television footer_icon2'></i>
								</div>

								<div class='pointer action nicetitle' onclick='toggle_tv()' title="Show/Hide TV. Hidden means it will not be displayed or active">
									<i class='fa fa-toggle-on footer_icon2' id='footer_toggle_tv_icon'></i>
								</div>

								<div class='pointer action nicetitle' onclick='toggle_lock_tv()' title="Lock/Unlock TV. Locked means it will stay the same even if media is changed by a user">
									<i class='fa fa-unlock-alt footer_icon' id='footer_lock_tv_icon'></i>
								</div>

 							</div>

							<div id='footer_radio_controls' class='footer_media_control'>

								<div class='footer_text_small'>Radio</div>

								<div class='pointer action nicetitle' onclick='show_radio_picker()' title="Change the Radio">
									<i id='footer_radio_icon' class='fa fa-volume-up footer_icon2'></i>
								</div>

								<div class='pointer action nicetitle' onclick='toggle_radio()' title="Show/Hide Radio. Hidden means it will not be displayed or active">
									<i class='fa fa-toggle-on footer_icon2' id='footer_toggle_radio_icon'></i>
								</div>


								<div class='pointer action nicetitle' onclick='toggle_lock_radio()' title="Lock/Unlock Radio. Locked means it will stay the same even if media is changed by a user">
									<i class='fa fa-unlock-alt footer_icon' id='footer_lock_radio_icon'></i>
								</div>

							</div>

							<div class='pointer action' onclick='show_media_menu()'>
								<i id='footer_media_menu' class='fa fa-bars footer_icon' id='footer_lock_images_icon'></i>
							</div>
						</div>

					</div>
				</div>

			</div>

		</div>

		<script id='template_main_menu' type='text/x-handlebars-template'>
			<div id='main_menu' class='unselectable'>

				<div class='menu_item action inline' onclick="request_roomlist('', 'visited_roomlist')">Visited Rooms</div>
				<div></div>
				<div class='menu_item action inline' onclick="request_roomlist('', 'public_roomlist')">Public Rooms</div>
				<div></div>
				<div class='menu_item action inline' onclick="show_goto_room()">Go To Room</div>
				<div></div>
				<div class='menu_item action inline' onclick='show_create_room()'>Create Room</div>

				<div id='menu_smaller_container'>

					<span class='menu_item_smaller action' onclick='show_help(1)'>Help</span>

					<div class='spacer2'></div>

					<div class='menu_item_smaller action' onclick='copy_room_url()'>Copy Room URL</div>

					<div class='spacer2'></div>

					<span class='menu_item_smaller action' onclick='show_chat_search()'>Search</span>

					&nbsp;|&nbsp;

					<span class='menu_item_smaller action' onclick='show_status()'>Status</span>

				</div>

				<div id='admin_menu'>

					<div class='spacer3'></div>
					<div class='spacer2'></div>

					<div class='small_padding_bottom'>Images</div>

					<select id='admin_enable_images' class='modal_select'>
						<option value="enabled">Enabled</option>
						<option value="disabled">Disabled</option>
						<option value="locked">Locked</option>
					</select>

					<div class='spacer1'></div>
					
					<div class='small_padding_bottom'>TV</div>

					<select id='admin_enable_tv' class='modal_select'>
						<option value="enabled">Enabled</option>
						<option value="disabled">Disabled</option>
						<option value="locked">Locked</option>
					</select>

					<div class='spacer1'></div>

					<div class='small_padding_bottom'>Radio</div>

					<select id='admin_enable_radio' class='modal_select'>
						<option value="enabled">Enabled</option>
						<option value="disabled">Disabled</option>
						<option value="locked">Locked</option>
					</select>

					<div class='spacer1'></div>
					
					<div class='small_padding_bottom'>Voice Chat</div>

					<select id='admin_enable_voice_chat' class='modal_select'>
						<option value="enabled">Enabled</option>
						<option value="disabled">Disabled</option>
					</select>

					<div class='spacer1'></div>
					<div class='spacer3'></div>

					<div class='small_padding_bottom'>Theme</div>

					<input id='admin_theme'>

					<div class='spacer1'></div>

					<div class='small_padding_bottom'>Text Color Mode</div>

					<select class='modal_select' id='admin_text_color_mode_select'>
						<option value="automatic">Automatic</option>
						<option value="custom">Custom</option>
					</select>

					<div id='admin_text_color_container'>
						<div class='spacer1'></div>
						<div class='small_padding_bottom'>Text Color</div>
						<input id='admin_text_color'>
					</div>

					<div class='spacer1'></div>

					<div class='small_padding_bottom'>Background Mode</div>

					<select class='modal_select' id='admin_background_mode_select'>
						<option value="normal">Normal</option>
						<option value="tiled">Tiled</option>
						<option value="mirror">Mirror</option>
						<option value="solid">Solid</option>
					</select>
					

					<div id='admin_background_tile_dimensions_container'>
						
						<div class='spacer1'></div>

						<div class='small_padding_bottom'>Tile Dimensions</div>

						<input type='text' class='setting_input_text_small' id='admin_background_tile_dimensions' title='CSS background-size property (width and height). For example 200px 200px, 100px auto, auto 100px, or 100px 200px'>		

					</div>

					<div id='admin_background_image_container'>
						<div class='spacer1'></div>
						<img class='small_square_image pointer' id='admin_background_image' onclick='open_background_image_picker()'>
					</div>

					<div class='spacer1'></div>
					<div class='spacer3'></div>

					<div class='toggler_main_container'>

						<div class='toggler main_menu_toggle small_padding_bottom bold pointer action inline'>+ Voice 1 Permissions</div>

						<div class='toggler_container main_menu_toggle_container'>

							<div class='spacer3'></div>

							<div class='setting_title'>Can Chat</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice1_chat_permission" data-ptype="voice1_chat_permission">
							
							<div class='spacer1'></div>

							<div class='setting_title'>Can Change Images</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice1_images_permission" data-ptype="voice1_images_permission">
							
							<div class='spacer1'></div>

							<div class='setting_title'>Can Change the TV</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice1_tv_permission" data-ptype="voice1_tv_permission">
							
							<div class='spacer1'></div>

							<div class='setting_title'>Can Change the Radio</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice1_radio_permission" data-ptype="voice1_radio_permission">

							<div class='spacer1'></div>

							<div class='setting_title'>Can Voice Chat</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice1_voice_chat_permission" data-ptype="voice1_voice_chat_permission">

							<div class='spacer3'></div>

						</div>

					</div>
					
					<div class='spacer1'></div>

					<div class='toggler_main_container'>

						<div class='toggler main_menu_toggle small_padding_bottom bold pointer action inline'>+ Voice 2 Permissions</div>

						<div class='toggler_container main_menu_toggle_container'>

							<div class='spacer3'></div>

							<div class='setting_title'>Can Chat</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice2_chat_permission" data-vtype="v2" data-ptype="voice2_chat_permission">
							
							<div class='spacer1'></div>

							<div class='setting_title'>Can Change Images</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice2_images_permission" data-vtype="v2" data-ptype="voice2_images_permission">
							
							<div class='spacer1'></div>

							<div class='setting_title'>Can Change the TV</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice2_tv_permission" data-vtype="v2" data-ptype="voice2_tv_permission">
							
							<div class='spacer1'></div>

							<div class='setting_title'>Can Change the Radio</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice2_radio_permission" data-vtype="v2" data-ptype="voice2_radio_permission">

							<div class='spacer1'></div>

							<div class='setting_title'>Can Voice Chat</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice2_voice_chat_permission" data-ptype="voice2_voice_chat_permission">

							<div class='spacer3'></div>

						</div>

					</div>
					
					<div class='spacer1'></div>

					<div class='toggler_main_container'>

						<div class='toggler main_menu_toggle small_padding_bottom bold pointer action inline'>+ Voice 3 Permissions</div>

						<div class='toggler_container main_menu_toggle_container'>

							<div class='spacer3'></div>

							<div class='setting_title'>Can Chat</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice3_chat_permission" data-vtype="v3" data-ptype="voice3_chat_permission">
							
							<div class='spacer1'></div>

							<div class='setting_title'>Can Change Images</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice3_images_permission" data-vtype="v3" data-ptype="voice3_images_permission">
							
							<div class='spacer1'></div>

							<div class='setting_title'>Can Change the TV</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice3_tv_permission" data-vtype="v3" data-ptype="voice3_tv_permission">
							
							<div class='spacer1'></div>

							<div class='setting_title'>Can Change the Radio</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice3_radio_permission" data-vtype="v3" data-ptype="voice3_radio_permission">

							<div class='spacer1'></div>

							<div class='setting_title'>Can Voice Chat</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice3_voice_chat_permission" data-ptype="voice3_voice_chat_permission">

							<div class='spacer3'></div>

						</div>

					</div>

					<div class='spacer1'></div>

					<div class='toggler_main_container'>

						<div class='toggler main_menu_toggle small_padding_bottom bold pointer action inline'>+ Voice 4 Permissions</div>
						
						<div class='toggler_container main_menu_toggle_container'>
						
							<div class='spacer3'></div>

							<div class='setting_title'>Can Chat</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice4_chat_permission" data-vtype="v4" data-ptype="voice4_chat_permission">
							
							<div class='spacer1'></div>

							<div class='setting_title'>Can Change Images</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice4_images_permission" data-vtype="v4" data-ptype="voice4_images_permission">
							
							<div class='spacer1'></div>

							<div class='setting_title'>Can Change the TV</div>
							<input type="checkbox" class="admin_voice_permissions_checkbox" id="admin_voice4_tv_permission" data-vtype="v4" data-ptype="voice4_tv_permission">
							
							<div class='spacer1'></div>

