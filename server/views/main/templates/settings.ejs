<script id='template_settings' type='text/x-handlebars-template' class='template'>
    <div id="settings_window_{{type}}" class='settings_main_window' data-settings_type='{{type}}'>

        <div class="settings_window_left" id="settings_window_left_{{type}}">
            <div class='settings_window_left_content' id='settings_window_left_content_{{type}}'>

                <div id='settings_window_category_afk_{{type}}' class='settings_window_category' data-category_container='settings_window_category_content_afk_{{type}}' data-category='afk'>
                    <span class='settings_window_category_status_filler invisible'></span>
                    <span class='settings_window_category_text'>AFK</span>
                    <span class='settings_window_category_status'></span>
                </div>

                <div id='settings_window_category_sound_{{type}}' class='settings_window_category' data-category_container='settings_window_category_content_sound_{{type}}' data-category='sound'>
                    <span class='settings_window_category_status_filler invisible'></span>
                    <span class='settings_window_category_text'>Sound</span>
                    <span class='settings_window_category_status'></span>
                </div>

                <div id='settings_window_category_appearance_{{type}}' class='settings_window_category' data-category_container='settings_window_category_content_appearance_{{type}}' data-category='appearance'>
                    <span class='settings_window_category_status_filler invisible'></span>
                    <span class='settings_window_category_text'>Appearance</span>
                    <span class='settings_window_category_status'></span>
                </div>

                <div id='settings_window_category_functions_{{type}}' class='settings_window_category' data-category_container='settings_window_category_content_functions_{{type}}' data-category='functions'>
                    <span class='settings_window_category_status_filler invisible'></span>
                    <span class='settings_window_category_text'>Functions</span>
                    <span class='settings_window_category_status'></span>
                </div>

                <div id='settings_window_category_media_{{type}}' class='settings_window_category' data-category_container='settings_window_category_content_media_{{type}}' data-category='media'>
                    <span class='settings_window_category_status_filler invisible'></span>
                    <span class='settings_window_category_text'>Media</span>
                    <span class='settings_window_category_status'></span>
                </div>

                <div id='settings_window_category_synth_{{type}}' class='settings_window_category' data-category_container='settings_window_category_content_synth_{{type}}' data-category='synth'>
                    <span class='settings_window_category_status_filler invisible'></span>
                    <span class='settings_window_category_text'>Synth</span>
                    <span class='settings_window_category_status'></span>
                </div>

                <div id='settings_window_category_misc_{{type}}' class='settings_window_category' data-category_container='settings_window_category_content_misc_{{type}}' data-category='misc'>
                    <span class='settings_window_category_status_filler invisible'></span>
                    <span class='settings_window_category_text'>Misc</span>
                    <span class='settings_window_category_status'></span>
                </div>

                <div class='settings_reset_button smaller_font_size pointer' onclick='Hue.confirm_reset_settings("{{type}}")'>Reset</div>

            </div>
        </div>

        <div class="settings_window_right" id="settings_window_right_{{type}}">

            <div class='settings_category settings_window_category_container' id='settings_window_category_content_sound_{{type}}' data-category='sound'>

                <div class='settings_item settings_top_level_item' data-setting='beep_on_messages'>
                    <div class="setting_title">Beep On Messages</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_beep_on_messages">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='beep_on_highlights'>
                    <div class="setting_title">Beep On Highlights</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_beep_on_highlights">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='beep_on_media_change'>
                    <div class="setting_title">Beep On Media Change</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_beep_on_media_change">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='beep_on_user_joins'>
                    <div class="setting_title">Beep On User Joins</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_beep_on_user_joins">
                </div>

            </div>

            <div class='settings_category settings_window_category_container' id='settings_window_category_content_appearance_{{type}}' data-category='appearance'>

                <div class='settings_item settings_top_level_item' data-setting='theme_mode'>
                    <div class="setting_title">Theme Mode</div>
                    <select class="{{type}}_settings_item_control modal_select" id="{{type}}_theme_mode">
                        <option value='room'>Room Theme</option>
                        <option value='custom'>Custom Theme</option>
                    </select>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='theme_color'>
                    <div class="setting_title">Theme Color</div>
                    <input type="color" class="{{type}}_settings_item_control setting_color" id="{{type}}_theme_color">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='text_color'>
                    <div class="setting_title">Text Color</div>
                    <input type="color" class="{{type}}_settings_item_control setting_color" id="{{type}}_text_color">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='background_mode'>
                    <div class="setting_title">Background Mode</div>
                    <select class="{{type}}_settings_item_control modal_select" id="{{type}}_background_mode">
                        <option value='room'>Room Background</option>
                        <option value='custom'>Custom Background</option>
                        <option value='disabled'>Disabled</option>
                    </select>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='background_url'>
                    <div class="setting_title">Background URL</div>
                    <input type="text" class="{{type}}_settings_item_control setting_text" id="{{type}}_background_url">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='animate_scroll'>
                    <div class="setting_title">Animate Scroll</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_animate_scroll">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='activity_bar'>
                    <div class="setting_title">Show Activity Bar</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_activity_bar">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='show_image_previews'>
                    <div class="setting_title">Show Image Previews</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_show_image_previews">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='show_link_previews'>
                    <div class="setting_title">Show Link Previews</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_show_link_previews">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='show_input_placeholder'>
                    <div class="setting_title">Show Input Placeholder</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_show_input_placeholder">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='show_clock_in_input_placeholder'>
                    <div class="setting_title2">Show Clock</div>
                    <div class="setting_title">In Input Placeholder</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_show_clock_in_input_placeholder">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='show_clock_in_lockscreen'>
                    <div class="setting_title2">Show Clock</div>
                    <div class="setting_title">In Lock Screen</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_show_clock_in_lockscreen">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='autoreveal_spoilers'>
                    <div class="setting_title">Auto-Reveal Spoilers</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_autoreveal_spoilers">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='chat_font_size'>
                    <div class="setting_title">Chat Font Size</div>
                    <select class="{{type}}_settings_item_control modal_select" id="{{type}}_chat_font_size">
                        <option value='very_small'>Very Small</option>
                        <option value='small'>Small</option>
                        <option value='normal'>Normal</option>
                        <option value='big'>Big</option>
                        <option value='very_big'>Very Big</option>
                    </select>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='chat_display_percentage'>
                    <div class="setting_title">Chat/Media Percentages</div>
                    <div class='slider_container'>
                        <div class='slider_icon_left pointer action' onclick='Hue.decrease_media_percentage()'><i class='icon2 fa fa-pencil'></i></div>
                        <input type='range' id='{{type}}_chat_display_percentage' min="10" max="90" step="10" ondblclick='Hue.set_default_media_size()'>
                        <div class='slider_icon_right pointer action' onclick='Hue.increase_media_percentage()'><i class='icon2b fa fa-picture-o'></i></div>
                    </div>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='tv_display_percentage'>
                    <div class="setting_title">Image/TV Percentages</div>
                    <div class='slider_container'>
                        <div class='slider_icon_left pointer action' onclick='Hue.decrease_tv_percentage()'><i class='icon2 fa fa-camera'></i></div>
                        <input type='range' id='{{type}}_tv_display_percentage' min="10" max="90" step="10" ondblclick='Hue.set_default_tv_size()'>
                        <div class='slider_icon_right pointer action' onclick='Hue.increase_tv_percentage()'><i class='icon2b fa fa-television'></i></div>
                    </div>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='tv_display_position'>
                    <div class="setting_title">Image/TV Positions</div>
                    <div id='{{type}}_tv_display_position' class='squaro_container pointer action2'>
                        <div class='squaro squaro_secondary' id='{{type}}_display_position_image'>
                            <i class='fa fa-camera smaller_font_size'></i>
                        </div>
                        <div class='squaro squaro_main' id='{{type}}_display_position_tv'>
                            <i class='fa fa-television smaller_font_size'></i>
                        </div>
                    </div>
                </div>

            </div>

            <div class='settings_category settings_window_category_container_selected' id='settings_window_category_content_afk_{{type}}' data-category='afk'>

                <div class='settings_item settings_top_level_item' data-setting='afk_delay'>
                    <div class="setting_title">Go AFK On App Onfocus</div>
                    <select class="{{type}}_settings_item_control modal_select" id="{{type}}_afk_delay">
                        <option value='0'>Instantly</option>
                        <option value='60000'>After 1 Minute</option>
                        <option value='120000'>After 2 Minutes</option>
                        <option value='300000'>After 5 Minutes</option>
                        <option value='600000'>After 10 Minutes</option>
                        <option value='1200000'>After 20 Minutes</option>
                        <option value='1800000'>After 30 Minutes</option>
                        <option value='3600000'>After 1 Hour</option>
                        <option value='never'>Never</option>
                    </select>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='afk_disable_messages_beep'>
                    <div class="setting_title2">On AFK</div>
                    <div class="setting_title">Disable Messages Beep</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_afk_disable_messages_beep">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='afk_disable_highlights_beep'>
                    <div class="setting_title2">On AFK</div>
                    <div class="setting_title">Disable Highlights Beep</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_afk_disable_highlights_beep">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='afk_disable_media_change_beep'>
                    <div class="setting_title2">On AFK</div>
                    <div class="setting_title">Disable Media Change Beep</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_afk_disable_media_change_beep">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='afk_disable_joins_beep'>
                    <div class="setting_title2">On AFK</div>
                    <div class="setting_title">Disable Joins Beep</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_afk_disable_joins_beep">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='afk_disable_image_change'>
                    <div class="setting_title2">On AFK</div>
                    <div class="setting_title">Disable Image Change</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_afk_disable_image_change">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='afk_disable_tv_change'>
                    <div class="setting_title2">On AFK</div>
                    <div class="setting_title">Disable TV Change</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_afk_disable_tv_change">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='afk_disable_radio_change'>
                    <div class="setting_title2">On AFK</div>
                    <div class="setting_title">Disable Radio Change</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_afk_disable_radio_change">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='afk_disable_synth'>
                    <div class="setting_title2">On AFK</div>
                    <div class="setting_title">Disable Synth</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_afk_disable_synth">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='afk_disable_notifications'>
                    <div class="setting_title2">On AFK</div>
                    <div class="setting_title">Disable Notifications</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_afk_disable_notifications">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='afk_on_lockscreen'>
                    <div class="setting_title2">Go AFK</div>
                    <div class="setting_title">On Lock Screen</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_afk_on_lockscreen">
                </div>

            </div>

            <div class='settings_category settings_window_category_container' id='settings_window_category_content_functions_{{type}}' data-category='functions'>

                <div id='settings_user_functions_container_{{type}}'>{{{user_functions}}}</div>

                <div class='settings_item settings_top_level_item' data-setting='double_tap'>
                    <div class='toggler_main_container'>
                        <div class="{{type}}_toggle toggler action pointer bold inline">+ On Double&nbsp;<span class='underlined' id="{{type}}_double_tap_key"></span>&nbsp;Tap</div>
                        <div class='{{type}}_toggle_container toggler_container'>
                            <div class="setting_title3">(Each on a different line)</div>
                            <textarea rows=5 class="{{type}}_settings_item_control setting_textarea3" id="{{type}}_double_tap"></textarea>
                        </div>
                    </div>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='double_tap_2'>
                    <div class='toggler_main_container'>
                        <div class="{{type}}_toggle toggler action pointer bold inline">+ On Double&nbsp;<span class='underlined' id="{{type}}_double_tap_2_key"></span>&nbsp;Tap</div>
                        <div class='{{type}}_toggle_container toggler_container'>
                            <div class="setting_title3">(Each on a different line)</div>
                            <textarea rows=5 class="{{type}}_settings_item_control setting_textarea3" id="{{type}}_double_tap_2"></textarea>
                        </div>
                    </div>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='double_tap_3'>
                    <div class='toggler_main_container'>
                        <div class="{{type}}_toggle toggler action pointer bold inline">+ On Double&nbsp;<span class='underlined' id="{{type}}_double_tap_3_key"></span>&nbsp;Tap</div>
                        <div class='{{type}}_toggle_container toggler_container'>
                            <div class="setting_title3">(Each on a different line)</div>
                            <textarea rows=5 class="{{type}}_settings_item_control setting_textarea3" id="{{type}}_double_tap_3"></textarea>
                        </div>
                    </div>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='on_lockscreen'>
                    <div class='toggler_main_container'>
                        <div class="{{type}}_toggle toggler action pointer bold inline">+ On Lock Screen</div>
                        <div class='{{type}}_toggle_container toggler_container'>
                            <div class="setting_title3">(Each on a different line)</div>
                            <textarea rows=5 class="{{type}}_settings_item_control setting_textarea3" id="{{type}}_on_lockscreen"></textarea>
                        </div>
                    </div>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='on_unlockscreen'>
                    <div class='toggler_main_container'>
                        <div class="{{type}}_toggle toggler action pointer bold inline">+ On Unlock Screen</div>
                        <div class='{{type}}_toggle_container toggler_container'>
                            <div class="setting_title3">(Each on a different line)</div>
                            <textarea rows=5 class="{{type}}_settings_item_control setting_textarea3" id="{{type}}_on_unlockscreen"></textarea>
                        </div>
                    </div>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='at_startup'>
                    <div class='toggler_main_container'>
                        <div class="{{type}}_toggle toggler action pointer bold inline">+ At Startup</div>
                        <div class='{{type}}_toggle_container toggler_container'>
                            <div class="setting_title3">(Each on a different line)</div>
                            <textarea rows=5 class="{{type}}_settings_item_control setting_textarea3" id="{{type}}_at_startup"></textarea>
                        </div>
                    </div>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='aliases'>
                    <div class='toggler_main_container'>
                        <div class="{{type}}_toggle toggler action pointer bold inline" title="For example /sky = /tv touch the sky. If the alias starts with an X, for example, /X1 = /radio restart, it won't be saved to history when used">+ Aliases</div>
                        <div class='{{type}}_toggle_container toggler_container'>
                            <div class="setting_title3">(Each on a different line)</div>
                            <textarea rows=5 class="{{type}}_settings_item_control setting_textarea3" id="{{type}}_aliases"></textarea>
                        </div>
                    </div>
                </div>

            </div>

            <div class='settings_category settings_window_category_container' id='settings_window_category_content_media_{{type}}' data-category='media'>

                <div class='settings_item settings_top_level_item' data-setting='stop_radio_on_tv_play'>
                    <div class="setting_title2">Stop Radio</div>
                    <div class="setting_title">On TV Play</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_stop_radio_on_tv_play">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='stop_tv_on_radio_play'>
                    <div class="setting_title2">Stop TV</div>
                    <div class="setting_title">On Radio Play</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_stop_tv_on_radio_play">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='bypass_images_lock_on_own_change'>
                    <div class="setting_title2">Bypass Images Lock</div>
                    <div class="setting_title">When You Change</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_bypass_images_lock_on_own_change">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='bypass_tv_lock_on_own_change'>
                    <div class="setting_title2">Bypass TV Lock</div>
                    <div class="setting_title">When You Change</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_bypass_tv_lock_on_own_change">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='bypass_radio_lock_on_own_change'>
                    <div class="setting_title2">Bypass Radio Lock</div>
                    <div class="setting_title">When You Change</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_bypass_radio_lock_on_own_change">
                </div>

            </div>

            <div class='settings_category settings_window_category_container' id='settings_window_category_content_synth_{{type}}' data-category='synth'>

                <div class='settings_item settings_top_level_item' data-setting='synth_enabled'>
                    <div class="setting_title">Synth Enabled</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_synth_enabled">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='speech_1'>
                    <div class="setting_title3" title="This is spoken when middle clicking key #1">Speech #1</div>
                    <textarea rows=2 class="{{type}}_settings_item_control setting_textarea2" id="{{type}}_speech_1"></textarea>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='speech_2'>
                    <div class="setting_title3" title="This is spoken when middle clicking key #2">Speech #2</div>
                    <textarea rows=2 class="{{type}}_settings_item_control setting_textarea2" id="{{type}}_speech_2"></textarea>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='speech_3'>
                    <div class="setting_title3" title="This is spoken when middle clicking key #3">Speech #3</div>
                    <textarea rows=2 class="{{type}}_settings_item_control setting_textarea2" id="{{type}}_speech_3"></textarea>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='speech_4'>
                    <div class="setting_title3" title="This is spoken when middle clicking key #4">Speech #4</div>
                    <textarea rows=2 class="{{type}}_settings_item_control setting_textarea2" id="{{type}}_speech_4"></textarea>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='speech_5'>
                    <div class="setting_title3" title="This is spoken when middle clicking key #5">Speech #5</div>
                    <textarea rows=2 class="{{type}}_settings_item_control setting_textarea2" id="{{type}}_speech_5"></textarea>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='speech_6'>
                    <div class="setting_title3" title="This is spoken when middle clicking key #6">Speech #6</div>
                    <textarea rows=2 class="{{type}}_settings_item_control setting_textarea2" id="{{type}}_speech_6"></textarea>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='speech_7'>
                    <div class="setting_title3" title="This is spoken when middle clicking key #7">Speech #7</div>
                    <textarea rows=2 class="{{type}}_settings_item_control setting_textarea2" id="{{type}}_speech_7"></textarea>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='speech_8'>
                    <div class="setting_title3" title="This is spoken when middle clicking key #8">Speech #8</div>
                    <textarea rows=2 class="{{type}}_settings_item_control setting_textarea2" id="{{type}}_speech_8"></textarea>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='speech_9'>
                    <div class="setting_title3" title="This is spoken when middle clicking key #9">Speech #9</div>
                    <textarea rows=2 class="{{type}}_settings_item_control setting_textarea2" id="{{type}}_speech_9"></textarea>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='speech_10'>
                    <div class="setting_title3" title="This is spoken when middle clicking key #10">Speech #10</div>
                    <textarea rows=2 class="{{type}}_settings_item_control setting_textarea2" id="{{type}}_speech_10"></textarea>
                </div>

            </div>

            <div class='settings_category settings_window_category_container' id='settings_window_category_content_misc_{{type}}' data-category='misc'>

                <div class='settings_item settings_top_level_item' data-setting='show_joins'>
                    <div class="setting_title">Show User Joins</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_show_joins">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='show_parts'>
                    <div class="setting_title">Show User Parts</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_show_parts">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='warn_before_closing'>
                    <div class="setting_title">Warn On Close</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_warn_before_closing">
                </div>

                <div class='settings_item settings_top_level_item' data-setting='open_popup_messages'>
                    <div class="setting_title2">Open Popup Messages</div>
                    <div class="setting_title">Automatically</div>
                    <input type="checkbox" class="{{type}}_settings_item_control setting_checkbox" id="{{type}}_open_popup_messages">
