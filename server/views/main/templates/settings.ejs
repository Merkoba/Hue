                    <div class='toggler_main_container'>
                        <div class="{{type}}_toggle toggler action pointer bold inline">+ Words To Autocomplete</div>
                        <div class='{{type}}_toggle_container toggler_container'>
                            <div class="setting_title3">(Each on a different line)</div>
                            <textarea rows=5 class="{{type}}_settings_item_control setting_textarea3" id="{{type}}_other_words_to_autocomplete"></textarea>
                        </div>
                    </div>
                </div>

                <div class='settings_item settings_top_level_item' data-setting='ignored_usernames'>
                    <div class='toggler_main_container'>
                        <div class="{{type}}_toggle toggler action pointer bold inline">+ Usernames To Ignore</div>
